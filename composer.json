{
    "name": "amritms/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": [
        "laravel",
        "braintree",
        "billing",
        "cashier"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Amrit Man Shrestha",
            "email": "amritms@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "braintree/braintree_php": "~4.6",
        "dompdf/dompdf": "^v0.8.5",
        "illuminate/contracts": "~5.7.0|~5.8.0|~5.9.0|^6.0|^7.0",
        "illuminate/database": "~5.7.0|~5.8.0|~5.9.0|^6.0|^7.0",
        "illuminate/http": "~5.7.0|~5.8.0|~5.9.0|^6.0|^7.0",
        "illuminate/routing": "~5.7.0|~5.8.0|~5.9.0|^6.0|^7.0",
        "illuminate/support": "~5.7.0|~5.8.0|~5.9.0|^6.0|^7.0",
        "illuminate/view": "~5.7.0|~5.8.0|~5.9.0|^6.0|^7.0",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "~4.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
